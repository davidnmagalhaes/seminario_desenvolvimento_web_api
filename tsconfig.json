{
  "ts-node": {
    "require": ["tsconfig-paths/register"]
},  
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "./src",
  },
  "include": ["src/**/*.ts", "src/models/interface/**/*.ts"],
}